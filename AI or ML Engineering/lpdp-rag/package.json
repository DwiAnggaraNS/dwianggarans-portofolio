{
  "name": "magang-berdampak-rag",
  "version": "1.0.0",
  "description": "RAG Website for Magang Berdampak Program",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/input.css -o ./static/css/style.css --watch",
    "build-css-prod": "tailwindcss -i ./static/css/input.css -o ./static/css/style.css --minify",
    "dev": "concurrently \"npm run build-css\" \"python app.py\"",
    "start": "python app.py"
  },
  "keywords": [
    "flask",
    "rag",
    "ai",
    "magang",
    "education",
    "tailwindcss"
  ],
  "author": "Magang Berdampak Team",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "concurrently": "^8.2.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10"
  }
}
